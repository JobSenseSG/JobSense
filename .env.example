# Database connection string for TimescaleDB
DATABASE_URL=postgres://<username>:<password>@<hostname>:<port>/<database>?sslmode=require

# AWS credentials for accessing services
AWS_ACCESS_KEY_ID=<your-aws-access-key-id>
AWS_SECRET_ACCESS_KEY=<your-aws-secret-access-key>
AWS_REGION=<your-aws-region>

# API key for Predibase
PREDIBASE_API_KEY=<your-predibase-api-key>

# API key for Upstage AI
UPSTAGE_API_KEY=<your-upstage-api-key>

# Supabase credentials
SUPABASE_URL=<your-supabase-url>
SUPABASE_KEY=<your-supabase-key>